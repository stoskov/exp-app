{
    "compilerOptions": {
        "target": "es5",
        "module": "system",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false
    },
    "exclude": [
        "angular2",
        "es6-promise",
        "es6-shim",
        "rxjs",
        "systemjs",
        "typescript",
        "reflect-metadata"
  ]
}
